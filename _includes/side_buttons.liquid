{% include_cached cache/side_buttons.liquid %}

{% if page.previous_site %}
<a href="{{ page.previous_site | relative_url }}" class="side-button" id="left-button">
  <img src="/assets/icons/left.svg" width="50" height="50">
</a>
{% endif %}

{% if page.next_site %}
<a href="{{ page.next_site | relative_url }}" class="side-button" id="right-button">
  <img src="/assets/icons/right.svg" width="50" height="50">
</a>
{% endif %}
